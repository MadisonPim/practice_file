let sigma = prompt("Are you sigma?!").toLowerCase(); 

if (sigma === "yes") {
    alert("Very Sigma");
} else if (sigma === "no") {
    alert("Not Sigma");
} else {
    alert("Please type 'yes' or 'no'.");
}
